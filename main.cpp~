#include <iostream>
#include <string>
#include <vector>

#include "obra.h"
#include "literatura.h"
#include "diseno.h"
#include "pintura.h"

using std::cin;
using std::cout;
using std::endl;
using std::string;
using std::vector;

int main(int argc,char*argv[]){
	vector<Obra> inventario;
	//inventario.push_back(Obra("n","a","f"));
	Literatura b("n","a","f","c","d");

	inventario.push_back(b);
	cout << inventario.size() << endl;
	for(int i=0; i<inventario.size(); i++){
		cout << i;
		inventario[i].toString();
	}
	return 0;
}
